#include <iostream>
using namespace std;

int main() {
	setlocale(0, "");

	cout << "Введите число от 1 до 100: ";
	int number;
	cin >> number;

	if (number >= 0 and number <= 10) {
		cout << "Цифра в диапазоне: 0-10";
	}
	else if (number >= 11 and number <= 20) {
		cout << "Цифра в диапазоне: 11-20";
	}
	else if (number >= 21 and number <= 30) {
		cout << "Цифра в диапазоне: 21-30";
	}
	else if (number >= 31 and number <= 40) {
		cout << "Цифра в диапазоне: 31-40";
	}
	else if (number >= 41 and number <= 50) {
		cout << "Цифра в диапазоне: 41-50";
	}
	else if (number >= 51 and number <= 60) {
		cout << "Цифра в диапазоне: 51-60";
	}
	else if (number >= 61 and number <= 70) {
		cout << "Цифра в диапазоне: 61-79";
	}
	else if (number >= 71 and number <= 80) {
		cout << "Цифра в диапазоне: 71-80";
	}
	else if (number >= 81 and number <= 90) {
		cout << "Цифра в диапазоне: 81-90";
	}
	else if (number >= 91 and number <= 100) {
		cout << "Цифра в диапазоне: 91-100";
	}
	else if (number < 0) {
		cout << "Число меньше 0";
	}
	else if (number > 100) {
		cout << "Число больше 100";
	}

	int _;
	cin >> _;
}
